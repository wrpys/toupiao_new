<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.model.TRescueMethod" table="t_rescue_method">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="contentInfo" type="java.lang.String">
			<column name="content_info" length="50" />
		</property>
		<property name="methodName" type="java.lang.String">
			<column name="method_Name" length="50" />
		</property>
		<property name="createTime" type="java.lang.String">
			<column name="create_time" length="50" />
		</property>
		
	</class>
</hibernate-mapping>
